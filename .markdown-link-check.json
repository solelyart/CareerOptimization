{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "\\[Your "
    },
    {
      "pattern": "\\[Date\\]"
    },
    {
      "pattern": "\\[your-"
    },
    {
      "pattern": "^\\.git/"
    },
    {
      "pattern": "linkedin\\.com"
    },
    {
      "pattern": "flexjobs\\.com"
    },
    {
      "pattern": "belaysolutions\\.com"
    },
    {
      "pattern": "theprofessorisin\\.com"
    },
    {
      "pattern": "Contractor_Evaluation_Rubric\\.md"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://www.linkedin.com", "https://linkedin.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate",
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "10s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308, 403, 429]
}
